  "main": "dist/lib/index.js",
  "module": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "bin": {
    "slack-exporter": "dist/main.js"
  },
  "files": [
    "dist",
    "src",
    "tsconfig.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "main": "ts-node src/main.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
