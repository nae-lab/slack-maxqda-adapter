    "@types/yargs": "^17.0.33",
    "@vitest/ui": "^3.1.4",
    "c8": "^10.1.3",
    "dotenv": "^16.5.0",
    "happy-dom": "^17.5.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
